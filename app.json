{
  "expo": {
    "name": "Cammove",
    "slug": "frontend-cammove",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_1024.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_name_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#EEE1FF"
    },
    "ios": {
      "bundleIdentifier": "com.camilafernandessilva.cammove",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Precisamos de acesso à sua câmera para tirar fotos.",
        "NSPhotoLibraryUsageDescription": "Precisamos de acesso à sua galeria para selecionar imagens.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1"
    },
    "android": {
      "package": "com.camilafernandessilva.cammove",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_1024.png",
        "backgroundColor": "#EEE1FF"
      },
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo_1024.png",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#EEE1FF",
          "image": "./assets/images/logo_name_1024.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "react-native-video"
    ],
    "scheme": "cammove",
    "extra": {
      "eas": {
        "projectId": "ce64223f-77d5-480a-abd6-aafe0cc90a5f"
      },
      "router": {
        "origin": false
      }
    },
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    }
  }
}
